<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/kerja_nav_graph"
    app:startDestination="@id/kuliahFragment">

    <fragment
        android:id="@+id/kuliahFragment"
        android:name="com.kelompok2.aplikasiplanme.KuliahFragment"
        android:label="fragment_kuliah"
        tools:layout="@layout/fragment_kuliah" >
        <action
            android:id="@+id/action_kuliahFragment_to_kerjaFragment"
            app:destination="@id/kerjaFragment" />
    </fragment>
    <fragment
        android:id="@+id/kerjaFragment"
        android:name="com.kelompok2.aplikasiplanme.KerjaFragment"
        android:label="fragment_kerja"
        tools:layout="@layout/fragment_kerja" >
        <action
            android:id="@+id/action_kerjaFragment_to_assignWorkFragment"
            app:destination="@id/assignWorkFragment" />
    </fragment>
    <fragment
        android:id="@+id/assignWorkFragment"
        android:name="com.kelompok2.aplikasiplanme.AssignWorkFragment"
        android:label="fragment_assign_work"
        tools:layout="@layout/fragment_assign_work" />
</navigation>